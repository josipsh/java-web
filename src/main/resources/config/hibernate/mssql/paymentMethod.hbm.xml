<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="hr.algebra.models.PaymentMethod" table="PaymentMethod">
        <id name="id">
            <column name="Id"/>
            <generator class="identity"/>
        </id>

        <property name="type">
            <column name="TypeName"/>
        </property>
    </class>
</hibernate-mapping>